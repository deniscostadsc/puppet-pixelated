# This file is managed by Puppet, changes may be overwritten
if $syslogtag contains_i 'user_agent' then {
  action(type="omfile" file="/var/log/pixelated/user-agent.log")
  stop
}

if $programname == 'tornado.access'
  or $programname == 'pixelated.proxy'
  or $programname == 'pixelated.manager'
  then {
    action(type="omfile" file="/var/log/pixelated/dispatcher.log")
  stop
}

